{
  "name": "@hypericon/yuzu",
  "version": "0.1.0",
  "description": "Library to push UI State updates across connected clients in CI applications",
  "main": "dist/index.js",
  "author": "Ed Johnson, Hypericon Ltd",
  "license": "UNLICENSED",
  "homepage": "https://github.com/hypcn/yuzu#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypcn/yuzu.git"
  },
  "scripts": {
