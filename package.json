    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "dist"
  ]
